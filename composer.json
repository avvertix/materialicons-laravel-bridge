{
    "name": "avvertix/materialicons-laravel-bridge",
    "description": "Material Design Icons packaged for easy SVG inlining in Laravel Blade templates",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alessio",
            "email": "alessio@oneofftech.xyz"
        }
    ],
    "require": {
        "php": ">=7.0",
        "nothingworks/blade-svg": "0.2.*",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0"
    },

    "autoload": {
        "psr-4": {
            "MaterialIcons\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "mockery/mockery": "^1.1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaterialIcons\\MaterialIconsBridgeServiceProvider"
            ]
        }
    }
}
