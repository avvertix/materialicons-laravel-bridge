{
    "name": "avvertix/materialicons-laravel-bridge",
    "description": "Material Design Icons packaged for easy SVG inlining in Laravel Blade templates",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alessio",
            "email": "alessio@oneofftech.xyz"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "blade-ui-kit/blade-icons": "^0.5",
        "illuminate/support": "^7.6",
        "illuminate/filesystem": "^7.6"
    },

    "autoload": {
        "psr-4": {
            "MaterialIcons\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaterialIcons\\MaterialIconsBridgeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
